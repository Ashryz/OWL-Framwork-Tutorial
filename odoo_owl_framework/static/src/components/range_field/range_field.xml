<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="odoo_owl_framework.RangeFieldTemplate" owl="1">
       <div class="mb-3">
            <label for="customRange1" class="form-label"><span t-esc="props.value"/> <span t-esc="currency"/> </label>
           <input type="range" class="form-range" id="customRange1" min="100" max="10000" step="100" t-on-change="(e)=>this.props.update(e.target.value)" t-att-value="props.value"/>
        </div>
    </t>

</templates>
